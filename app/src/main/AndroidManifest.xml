<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eabiii.firebasetest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Register" />
        <activity android:name=".UserHomepage" />
        <activity android:name=".PostActivity" android:parentActivityName=".UserHomepage"/>
        <activity android:name=".ViewSinglePost" android:parentActivityName=".UserHomepage" />
        <activity android:name=".ViewPolitician" android:parentActivityName=".UserHomepage"/>
        <activity android:name=".RatePolitician" />
        <activity android:name=".AddPartyList" android:parentActivityName=".UserHomepage"/>
        <activity android:name=".AddPolitician" android:parentActivityName=".UserHomepage"/>
        <activity android:name=".ViewPartylistMember" android:parentActivityName=".UserHomepage"/>
        <activity android:name=".Settings" />
        <activity android:name=".ChangePassword"></activity>
    </application>

</manifest>